{
  "name": "@sunhwa/workspace",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:wordle": "npm run dev -w ./packages/wordle",
    "start:toast": "npm run dev -w ./packages/toast",
    "start:next": "npm run dev -w ./packages/nextjs",
    "start:nextbuild": "npm run build -w ./packages/nextjs",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "preact": "^10.5.15",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "@types/styled-components": "^5.1.21",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
